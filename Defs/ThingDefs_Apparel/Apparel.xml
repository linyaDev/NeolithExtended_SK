                    <female>
                        <offset>(0.2,0)</offset>
                    </female>
                </east>
                <west>
                    <offset>(0,-0.05)</offset>
                    <thin>
                        <offset>(-0.2,0)</offset>
                    </thin>
                    <hulk>
                        <offset>(-0.05,0)</offset>
                    </hulk>
                    <fat>
                        <offset>(0,0)</offset>
                    </fat>
                    <male>
                        <offset>(-0.2,0)</offset>
                    </male>
                    <female>
                        <offset>(-0.2,0)</offset>
